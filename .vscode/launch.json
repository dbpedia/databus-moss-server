{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "type": "java",
            "name": "Launch Moss Server",
            "request": "launch",
            "mainClass": "org.dbpedia.moss.Main",
            "projectName": "moss",
            "env": {
                "CONFIG_PATH": "./config/moss-config.yml",
                "MOSS_BASE_URL": "http://localhost:8080",
                "GSTORE_BASE_URL": "http://localhost:5003",
                "LOOKUP_BASE_URL": "http://localhost:5002",
                "STORE_SPARQL_ENDPOINT": "http://localhost:5004/sparql",
                "HTTPS_PROXY": null,
                "HTTP_PROXY": null,
                "USER_DATABASE_PATH": "./devenv/sqlite/users.db",
                "AUTH_OIDC_ISSUER": "https://auth.dbpedia.org/realms/dbpedia",
                "JWT_VERBOSE": true,
                "ADMIN_ROLE": "moss-dev-admin",
                "AUTH_OIDC_CLIENT_ID": "moss-dev"
            }
        }
    ]
}