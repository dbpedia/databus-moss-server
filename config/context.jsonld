{
    "@context": {
        "moss": "http://dataid.dbpedia.org/ns/moss#",
        "dct": "http://purl.org/dc/terms/",
        "xsd": "http://www.w3.org/2001/XMLSchema#",
        "name": "moss:name",
        "formatMimeType": "moss:mimeType",
        "extends": {
            "@id": "moss:extends",
            "@type": "@id"
        }, 
        "content": {
            "@id": "moss:content",
            "@type": "@id"
        }, 
        "instanceOf": {
            "@id": "moss:instanceOf",
            "@type": "@id"
        }, 
        "MetadataLayer": "moss:MetadataLayer",
        "MetadataEntry": "moss:MetadataEntry",
        "subClassOf": "http://www.w3.org/2000/01/rdf-schema#subClassOf",
        "Class": "http://www.w3.org/2000/01/rdf-schema#Class",
        "created": {
            "@id": "dct:created",
            "@type": "xsd:dateTime"
        },
        "modified": {
            "@id": "dct:modified",
            "@type": "xsd:dateTime"
        },
        "indexers": {
            "@id" : "moss:indexer",
            "@type": "@id",
            "@container": "@set"
        }
    }
}
